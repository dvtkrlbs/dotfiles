{
  "edit_predictions": {
    "mode": "subtle"
  },
  "context_servers": {},
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "agent": {
    "default_profile": "ask",
    "model_parameters": [],
    "default_model": {
      "provider": "copilot_chat",
      "model": "gemini-2.5-pro"
    },
    "version": "2"
  },
  "show_edit_predictions": true,
  "buffer_font_family": "JetBrains Mono",
  "theme": "Catppuccin Mocha",
  "icon_theme": "Catppuccin Mocha",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "vim_mode": true,
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "diagnostics": false
      }
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "agent_servers": {
    "OpenCode": {
      "command": "opencode",
      "args": ["acp"]
    }
  }
}
